# Control Panel title
#: build/index.js
msgid "Date prefix"
msgstr "Rhagddodiad y dyddiad"

# Toggle label
#: build/index.js
msgid "Show prefix before date"
msgstr "Dangos y rhagddodiad cyn y dyddiad"

# Example prefix (for Post Date)
#: build/index.js
msgid "Published"
msgstr "Cyhoeddwyd"

# Example prefix (for Modified Date)
#: build/index.js
msgid "Updated"
msgstr "Diweddarwyd"

# Help text (for Post Date)
#: build/index.js
msgid "Type the prefix inline before the date (e.g., %s)."
msgstr "Teipiwch y rhagddodiad yn syth cyn y dyddiad (e.e., %s)."

# Help text (for Modified Date)
#: build/index.js
msgid "Adds text (e.g. %s) before the post date."
msgstr "Yn ychwanegu testun (e.e. %s) cyn dyddiad y post."

# The prefix placeholder
#: build/index.js
msgid "Prefix…"
msgstr "Rhagddodiad…"
# QueryRangeFormatPicker > NonDefaultControls

# Default format (range)
#: build/index.js
msgid "Displaying %1$s – %2$s of %3$s"
msgstr "Yn dangos %1$s – %2$s o %3$s"

# Default format (single)
#: build/index.js
msgid "Displaying %1$s of %2$s"
msgstr "Yn dangos %1$s o %2$s"

# Alternative format (range)
#: build/index.js
msgid "Showing %1$s to %2$s of %3$s results"
msgstr "Yn dangos %1$s i %2$s o %3$s o ganlyniadau"

# Alternative format (single)
#: build/index.js
msgid "Showing %1$s of %2$s results"
msgstr "Yn dangos %1$s o %2$s"


