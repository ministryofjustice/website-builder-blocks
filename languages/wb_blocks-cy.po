# Labels for the custom format
#: build/index.js
msgid "Enter your own phrase format"
msgstr "Rhowch eich fformat ymadrodd eich hun"

#: build/index.js
msgid "Single item format"
msgstr "Fformat eitem sengl"

#: build/index.js
msgid "Range format"
msgstr "Fformat amrediad"


# Help text for the custom format
#: build/index.js
msgid "Use placeholders: %1$s (index), %2$s (total)."
msgstr "efnyddiwch ddewisyddion: %1$s (mynegai), %2$s (cyfanswm)."

#: build/index.js
msgid "Use placeholders: %1$s (start), %2$s (end), %3$s (total)."
msgstr "Defnyddiwch ddewisyddion: %1$s (cychwyn), %2$s (diwedd), %3$s (cyfanswm)."

#: build/index.js
msgid "For bold text use tags: <b> & </b>."
msgstr "Ar gyfer testun trwm defnyddiwch dagiau: <b> a </b>."


